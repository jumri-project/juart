{
    "name": "Jupyter Dev Container",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "features": {},
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter",
          "charliermarsh.ruff"
        ],
        "settings": {
          "python.languageServer": "Pylance",
          "jupyter.jupyterServerType": "local"
        }
      }
    },
    "remoteUser": "root"
}